# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'JSONBenchmarks' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for JSONBenchmarks
  pod 'HandyJSON', '~> 4.2.0'
  pod 'SwiftyJSON', '~> 4.0'  
  pod 'SwiftProtobuf', '~> 1.0'  
  pod 'PMJSON', '~> 3.0'
  pod 'JASON', '~> 3.0'
  pod 'ObjectMapper', '~> 3.4'
  pod 'Marshal', '~> 1.2.4'
  pod 'Tailor'

  target 'JSONBenchmarksTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'JSONBenchmarksUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
